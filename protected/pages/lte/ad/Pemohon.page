<%@ Title="Pemohon" %>
<com:TContent ID="moduleheader">
    <i class="fa fa-user-md"></i> PEMOHON
</com:TContent>
<com:TContent ID="csscontent">
    <link rel="stylesheet" type="text/css" href="<%=$this->page->theme->baseUrl%>/js/plugins/chosen-bootstrap/chosen.min.css" />
</com:TContent>
<com:TContent ID="modulebreadcrumb">        
    <li class="active">Pemohon</li>
</com:TContent>
<com:TContent ID="maincontent">    
    <com:TPanel Visible="<%= $this->getEditProcess(false) %>" CssClass="row">
        <com:TActiveHiddenField ID="hiddenidpemohon" />  
        <com:TActiveHiddenField ID="hiddenidcom" />
        <com:TWizard ID="editpemohonwizard" 
            CssClass="col-lg-12"
            ShowSideBar="false"                              
            StepStyle.CssClass="box box-solid box-primary"
            NavigationStyle.CssClass="btn-group btn-group-justified"
            OnCompleteButtonClick="editPemohonCompleted" 
            OnNextButtonClick="processEditNextButton">
            <prop:StartNavigationTemplate>                    
                <com:TLinkButton Text="Next" CommandName="NextStep" ValidationGroup="editPemohon" CssClass="btn btn-default" />                    
            </prop:StartNavigationTemplate>
            <prop:StepNavigationTemplate>                
                <com:TLinkButton Text="Next" CommandName="NextStep" ValidationGroup="editPemohon" CssClass="btn btn-default" />                
            </prop:StepNavigationTemplate>
            <prop:FinishNavigationTemplate>
                <com:TLinkButton Text="Complete" CommandName="Complete" ValidationGroup="editPemohon" CssClass="btn btn-default" />                
            </prop:FinishNavigationTemplate>
            <com:TWizardStep ID="EditDatapribadiPemohon" Title="Data Pribadi" AllowReturn="false">                                     
                <div class="box-header">
                    <h3 class='box-title'>
                        <i class='fa fa-plus'></i>
                        UBAH PEMOHON - PROFIL PEMOHON
                    </h3>
                    <div class="box-tools pull-right">
                        <button data-widget="collapse" class="btn btn-primary btn-sm" onclick="JavaScript:return false;">
                            <i class="fa fa-minus"></i>
                        </button>
                        <button data-widget="remove" class="btn btn-primary btn-sm" onclick="pageredirect('<%=$this->constructUrl('Pemohon',true)%>');return false;">
                            <i class="fa fa-times"></i>
                        </button>
                    </div>
                </div>
                <div class="box-body">                    
                    <div class="form-group">
                        <label class="col-sm-2 control-label">Status Pemohon :</label>
                        <div class="col-sm-4">
                            <div class="radio">
                                <label>
                                    <com:TRadioButton ID="rdEditStatusPemohonPerorangan" Checked="true" GroupName="EditStatusPemohon" /> Perorangan                                 
                                </label>
                            </div>
                            <div class="radio">
                                <label>
                                    <com:TRadioButton ID="rdEditStatusPemohonPerusahaan" GroupName="EditStatusPemohon"  /> Perusahaan
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="form-group" id="divKodePemohon">
                        <label class="col-sm-2 control-label">Kode Pemohon  :</label>
                        <div class="col-sm-10">
                            <com:TActiveTextBox ID="txtEditKodePemohon" Columns="4" MaxLength="4" Attributes.OnKeyUp="formatangka(this,true)" CssClass="form-control" Attributes.PlaceHolder="kode" Width="75px;" Enabled="false" />			                            
                            <com:TRequiredFieldValidator ID="FieldEditKodeValidator" ControlToValidate="txtEditKodePemohon" Display="Dynamic" ErrorMessage="Isi Kode Pemohon" ValidationGroup="editPemohon" FocusOnError="true" CssClass="has-error help-block">			
                                <prop:ClientSide.OnValidationError>
                                    $('divKodePemohon').addClassName('has-error');
                                    jQuery('#<%=$this->FieldEditKodeValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divKodePemohon').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                            <com:TCustomValidator ID="editKodePemohon" ControlToValidate="txtEditKodePemohon" Display="Dynamic" OnServerValidate="checkId" ValidationGroup="editPemohon" ErrorMessage="Kode Pemohon Yang Anda Inputkan Sudah Ada" FocusOnError="true" CssClass="has-error help-block">
                                <prop:ClientSide.OnValidationError>
                                    $('divKodePemohon').addClassName('has-error');
                                    jQuery('#<%=$this->editKodePemohon->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divKodePemohon').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TCustomValidator>                        
                        </div>
                    </div>
                    <div class="form-group" id="divNamaPemohon">
                        <label class="col-sm-2 control-label">Nama Pemohon  :</label>
                        <div class="col-xs-5">
                            <com:TTextBox ID="txtEditNamaPemohon" CssClass="form-control" Attributes.PlaceHolder="Nama Pemohon" />
                            <com:TRequiredFieldValidator ID="FieldEditNamaPemohonValidator" ControlToValidate="txtEditNamaPemohon" Display="Dynamic" ErrorMessage="Isi Nama Pemohon" ValidationGroup="editPemohon" FocusOnError="true" CssClass="has-error help-block">				
                                <prop:ClientSide.OnValidationError>
                                    $('divNamaPemohon').addClassName('has-error');
                                    jQuery('#<%=$this->FieldEditNamaPemohonValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divNamaPemohon').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                        </div>
                    </div>
                    <div class="form-group" id="divKTPPemohon">
                        <label class="col-sm-2 control-label">No. KTP Pemohon  :</label>
                        <div class="col-xs-2">
                            <com:TTextBox ID="txtEditNoKTP" CssClass="form-control" Attributes.PlaceHolder="Nomor KTP" Attributes.OnKeyUp="formatangka(this,true)" />
                            <com:TRequiredFieldValidator ID="FieldEditNoKTPValidator" ControlToValidate="txtEditNoKTP" Display="Dynamic" ErrorMessage="Isi Nomor KTP Pemohon" ValidationGroup="editPemohon" FocusOnError="true" CssClass="has-error help-block">
                                <prop:ClientSide.OnValidationError>
                                    $('divKTPPemohon').addClassName('has-error');
                                    jQuery('#<%=$this->FieldEditNoKTPValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divKTPPemohon').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                        </div>
                    </div>                    
                    <div class="form-group" id="divAlamatPemohon">
                        <label class="col-sm-2 control-label">Alamat Pemohon  :</label>
                        <div class="col-xs-7">
                            <com:TTextBox ID="txtEditAlamatPemohon" CssClass="form-control" Attributes.PlaceHolder="Alamat Pemohon" />
                            <com:TRequiredFieldValidator ID="FieldEditAlamatPemohonValidator" ControlToValidate="txtEditAlamatPemohon" Display="Dynamic" ErrorMessage="Isi Alamat Pemohon" ValidationGroup="editPemohon" FocusOnError="true" CssClass="has-error help-block">
                                <prop:ClientSide.OnValidationError>
                                    $('divAlamatPemohon').addClassName('has-error');
                                    jQuery('#<%=$this->FieldEditAlamatPemohonValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divAlamatPemohon').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                        </div>
                    </div>
                    <div class="form-group" id="divNoTelp">
                        <label class="col-sm-2 control-label">No. Telp Pemohon  :</label>
                        <div class="col-xs-2">
                            <com:TTextBox ID="txtEditNoTelp" CssClass="form-control" Attributes.PlaceHolder="Nomor Telepon" Attributes.OnKeyUp="formatangka(this,true)" />
                            <com:TRequiredFieldValidator ID="FieldEditNoTelpValidator" ControlToValidate="txtEditNoTelp" Display="Dynamic" ErrorMessage="Isi Nomor Telp Pemohon" ValidationGroup="editPemohon" FocusOnError="true" CssClass="has-error help-block">
                                <prop:ClientSide.OnValidationError>
                                    $('divNoTelp').addClassName('has-error');
                                    jQuery('#<%=$this->FieldEditNoTelpValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divNoTelp').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                        </div>
                    </div>     
                    <div class="form-group" id="divNPWPPemohon">
                        <label class="col-sm-2 control-label">No. NPWP Pemohon  :</label>
                        <div class="col-xs-3">
                            <com:TTextBox ID="txtEditNoNPWPPemohon" CssClass="form-control" Attributes.PlaceHolder="Nomor NPWP Pemohon" Attributes.OnKeyUp="formatangka(this,true)" />                        
                        </div
                    </div>                    
                </div>
            </com:TWizardStep>
            <com:TWizardStep Title="Data Perusahaan" AllowReturn="false">                    
                <div class="box-header">
                    <h3 class='box-title'>
                        <i class='fa fa-plus'></i>
                        UBAH PEMOHON - PROFIL PERUSAHAAN
                    </h3>
                    <div class="box-tools pull-right">
                        <button data-widget="collapse" class="btn btn-primary btn-sm" onclick="JavaScript:return false;">
                            <i class="fa fa-minus"></i>
                        </button>
                        <button data-widget="remove" class="btn btn-primary btn-sm" onclick="pageredirect('<%=$this->constructUrl('Pemohon',true)%>');return false;">
                            <i class="fa fa-times"></i>
                        </button>
                    </div>
                </div>
                <div class="box-body">
                    <div class="form-group">
                        <label class="col-sm-2 control-label">Status Perusahaan  :</label>
                        <div class="col-sm-4">
                            <div class="radio">
                                <label>
                                    <com:TRadioButton ID="rdEditStatusPerusahaanPusat" Checked="true" GroupName="EditStatusPerusahaan" /> Pusat                                 
                                </label>
                            </div>
                            <div class="radio">
                                <label>
                                    <com:TRadioButton ID="rdEditStatusPerusahaanCabang" GroupName="EditStatusPerusahaan"  /> Cabang
                                </label>
                            </div>
                        </div>
                    </div>     
                    <div class="form-group" id="divNamaPerusahaan">
                        <label class="col-sm-2 control-label">Nama Perusahaan  :</label>
                        <div class="col-xs-5">
                            <com:TTextBox ID="txtEditNamaPerusahaan" CssClass="form-control" Attributes.PlaceHolder="Nama Perusahaan" />
                            <com:TRequiredFieldValidator ID="FieldEditNamaPerusahaanValidator" ControlToValidate="txtEditNamaPerusahaan" Display="Dynamic" ErrorMessage="Isi Nama Perusahaan" ValidationGroup="editPemohon" FocusOnError="true" CssClass="has-error help-block">				
                                <prop:ClientSide.OnValidationError>
                                    $('divNamaPerusahaan').addClassName('has-error');
                                    jQuery('#<%=$this->FieldEditNamaPerusahaanValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divNamaPerusahaan').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                        </div>
                    </div>
                    <div class="form-group" id="divNoAktePerusahaan">
                        <label class="col-sm-2 control-label">Nomor Akte Pendirian Perusahaan :</label>
                        <div class="col-xs-4">
                            <com:TTextBox ID="txtEditNoAktePerusahaan" CssClass="form-control" Width="200px" Attributes.PlaceHolder="No. Akte Perusahaan" />
                            <com:TRequiredFieldValidator ID="FieldEditNoAkteValidator" ControlToValidate="txtEditNoAktePerusahaan" Display="Dynamic" ErrorMessage="Isi Nomor Akte Perusahaan" ValidationGroup="editPemohon" FocusOnError="true" CssClass="has-error help-block">				
                                <prop:ClientSide.OnValidationError>
                                    $('divNoAktePerusahaan').addClassName('has-error');
                                    jQuery('#<%=$this->FieldEditNoAkteValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divNoAktePerusahaan').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-sm-2 control-label">Tanggal Akte Pendirian Perusahaan :</label>
                        <div class="col-xs-4">
                            <div class="radio">                                
                                <com:TCustomDatePicker ID="cmbEditTGLAktePendirian" DateFormat="dd-MMMM-yyyy" Culture="id"  FromYear="1970" UpToYear="<%= @date('Y') %>" InputMode="DropDownList" ShowCalendar="true" />                                                        
                            </div>
                        </div>
                    </div>
                    <div class="form-group" id="divNPWPPerusahaan">
                        <label class="col-sm-2 control-label">No. NPWP Perusahaan :</label>
                        <div class="col-xs-3">
                            <com:TTextBox ID="txtEditNoNPWP" CssClass="form-control" Attributes.PlaceHolder="Nomor NPWP Perusahaan" Attributes.OnKeyUp="formatangka(this,true)" />
                            <com:TRequiredFieldValidator ID="FieldEditNoNPWPValidator" ControlToValidate="txtEditNoNPWP" Display="Dynamic" ErrorMessage="Isi Nomor NPWP Perusahaan" ValidationGroup="editPemohon" FocusOnError="true" CssClass="has-error help-block">
                                <prop:ClientSide.OnValidationError>
                                    $('divNPWPPerusahaan').addClassName('has-error');
                                    jQuery('#<%=$this->FieldEditNoNPWPValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divNPWPPerusahaan').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                        </div>
                    </div>
                    <div class="form-group" id="divAlamatPerusahaan">
                        <label class="col-sm-2 control-label">Alamat Perusahaan :</label>
                        <div class="col-xs-5">
                            <com:TTextBox ID="txtEditAlamatPerusahaan" CssClass="form-control" Attributes.PlaceHolder="Alamat Perusahaan" />
                            <com:TRequiredFieldValidator ID="FieldEditAlamatPerusahaanValidator" ControlToValidate="txtEditAlamatPerusahaan" Display="Dynamic" ErrorMessage="Isi Alamat Perusahaan" ValidationGroup="editPemohon" FocusOnError="true" CssClass="has-error help-block">
                                <prop:ClientSide.OnValidationError>
                                    $('divAlamatPerusahaan').addClassName('has-error');
                                    jQuery('#<%=$this->FieldEditAlamatPerusahaanValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divAlamatPerusahaan').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                        </div>
                    </div>
                    <div class="form-group" id="divNoTelpPerusahaan">
                        <label class="col-sm-2 control-label">No. Telp Perusahaan :</label>
                        <div class="col-xs-2">
                            <com:TTextBox ID="txtEditNoTelpPerusahaan" CssClass="form-control" Attributes.PlaceHolder="Nomor Telp" Attributes.OnKeyUp="formatangka(this,true)" />
                            <com:TRequiredFieldValidator ID="FieldEditNoTelpPerusahaanValidator" ControlToValidate="txtEditNoTelpPerusahaan" Display="Dynamic" ErrorMessage="Isi Nomor Telp Perusahaan" ValidationGroup="editPemohon" FocusOnError="true" CssClass="has-error help-block">
                                <prop:ClientSide.OnValidationError>
                                    $('divNoTelpPerusahaan').addClassName('has-error');
                                    jQuery('#<%=$this->FieldEditNoTelpPerusahaanValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divNoTelpPerusahaan').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                        </div>
                    </div>     
                    <div class="form-group">
                        <label class="col-sm-2 control-label">No. Fax Perusahaan :</label>
                        <div class="col-xs-2">
                            <com:TTextBox ID="txtEditNoFaxPerusahaan" CssClass="form-control" Attributes.PlaceHolder="Nomor Fax" Attributes.OnKeyUp="formatangka(this,true)" />                        
                        </div>
                    </div> 
                    <div class="form-group">
                        <label class="col-sm-2 control-label">Alamat Kantor Cabang :</label>
                        <div class="col-xs-5">
                            <com:TTextBox ID="txtEditAlamatCabang" CssClass="form-control" Attributes.PlaceHolder="Alamat Kantor Cabang" />                        
                        </div>
                    </div>
                </div>
            </com:TWizardStep>                     
            <com:TWizardStep Title="Data Perusahaan" AllowReturn="false">                    
                <div class="box-header">
                    <h3 class='box-title'>
                        <i class='fa fa-plus'></i>
                        UBAH PEMOHON - FOTO PEMOHON (Maks. <%=$this->setup->formatSizeUnits(3148576)%>)
                    </h3>
                    <div class="box-tools pull-right">
                        <button data-widget="collapse" class="btn btn-primary btn-sm" onclick="JavaScript:return false;">
                            <i class="fa fa-minus"></i>
                        </button>
                        <button data-widget="remove" class="btn btn-primary btn-sm" onclick="pageredirect('<%=$this->constructUrl('Pemohon',true)%>');return false;">
                            <i class="fa fa-times"></i>
                        </button>
                    </div>
                </div>
                <div class="box-body">
                    <div class="form-group">
                        <label class="col-sm-2 control-label">Preview Foto Pemohon :</label>
                         <div class="col-sm-10">
                            <p class="form-control-static"><img src="<%=$this->editPath_userimages->Value%>" height="189" width="139" /></p>
                         </div>
                    </div>                                
                    <div class="form-group" id="divFile2">
                        <label class="col-sm-2 control-label">UPLOAD FOTO (ukuran 189x139) :</label>
                        <div class="col-xs-5">
                            <com:TFileUpload ID="editFileFoto" MaxFileSize="3148576"/>
                            <com:TCustomValidator ID="editFileFotoValidator" ControlToValidate="editFileFoto" Display="Dynamic" OnServerValidate="checkTypeFile" ValidationGroup="editPemohon" ErrorMessage="Ekstensi File yang di Ijinkan hanya jpg dan png." FocusOnError="true" CssClass="has-error help-block">
                                <prop:ClientSide.OnValidationError>
                                    $('divFile2').addClassName('has-error');
                                    jQuery('#<%=$this->addKodePemohon->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divFile2').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TCustomValidator>
                        </div>
                    </div> 
                </div>
            </com:TWizardStep> 
            <com:TWizardStep Title="Summary" AllowReturn="false" StepType="Complete">
                <com:THiddenField ID="editPath_userimages" />                
                <div class="box-header">
                    <h3 class='box-title'>
                        <i class='fa fa-plus'></i>
                        UBAH PEMOHON - RANGKUMAN
                    </h3>
                    <div class="box-tools pull-right">
                        <button data-widget="collapse" class="btn btn-primary btn-sm" onclick="JavaScript:return false;">
                            <i class="fa fa-minus"></i>
                        </button>
                        <button data-widget="remove" class="btn btn-primary btn-sm" onclick="pageredirect('<%=$this->constructUrl('Pemohon',true)%>');return false;">
                            <i class="fa fa-times"></i>
                        </button>
                    </div>
                </div>
                <div class="box-body">
                    <!-- Nav tabs -->
                    <ul class="nav nav-tabs">
                        <li class="active">
                            <a href="#profilpemohon" data-toggle="tab">PROFIL PEMOHON</a>
                        </li>
                        <li>
                            <a href="#profilperusahaan" data-toggle="tab">PROFIL PERUSAHAAN</a>
                        </li>
                        <li>
                            <a href="#fotopemohon" data-toggle="tab">FOTO PEMOHON</a>
                        </li>                           
                    </ul>
                    <!-- Tab panes -->
                    <div class="tab-content">
                        <div class="tab-pane fade in active" id="profilpemohon">
                            <h4>&nbsp;</h4>
                            <div class="row">
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">Status Pemohon :</label>
                                     <div class="col-sm-10">
                                        <p class="form-control-static"><%=$this->rdEditStatusPemohonPerorangan->Checked==true?'Perorangan':'Perusahaan'%></p>
                                     </div>
                                </div>                                
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">Kode Pemohon :</label>
                                     <div class="col-sm-10">
                                        <p class="form-control-static"><%=$this->txtEditKodePemohon->Text%></p>
                                     </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">Nama Pemohon :</label>
                                     <div class="col-sm-10">
                                        <p class="form-control-static"><%=$this->txtEditNamaPemohon->Text%></p>
                                     </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">No. KTP Pemohon :</label>
                                     <div class="col-sm-10">
                                        <p class="form-control-static"><%=$this->txtEditNoKTP->Text%></p>
                                     </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">Alamat Pemohon :</label>
                                     <div class="col-sm-10">
                                        <p class="form-control-static"><%=$this->txtEditAlamatPemohon->Text%></p>
                                     </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">No. Telp Pemohon :</label>
                                     <div class="col-sm-10">
                                        <p class="form-control-static"><%=$this->txtEditNoTelp->Text%></p>
                                     </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">NPWP Pemohon :</label>
                                     <div class="col-sm-10">
                                        <p class="form-control-static"><%=$this->txtEditNoNPWPPemohon->Text%></p>
                                     </div>
                                </div>
                            </div>                                                       
                        </div>
                        <com:TPanel CssClass="tab-pane fade" Attributes.id="profilperusahaan" Visible="<%=$this->rdEditStatusPemohonPerorangan->Checked==true%>">
                            <h4>&nbsp;</h4> 
                            <div class="alert alert-warning">                                
                                <p>Tidak ada data perusahaan.</p>
                            </div>    
                        </com:TPanel>
                        <com:TPanel CssClass="tab-pane fade" Attributes.id="profilperusahaan" Visible="<%=$this->rdEditStatusPemohonPerusahaan->Checked==true%>">
                            <h4>&nbsp;</h4>                            
                            <div class="row">
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">Nama Perusahaan :</label>
                                     <div class="col-sm-10">
                                        <p class="form-control-static"><%=$this->txtEditNamaPerusahaan->Text%></p>
                                     </div>
                                </div>                            
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">Nomor Akte Perusahaan :</label>
                                     <div class="col-sm-10">
                                        <p class="form-control-static"><%=$this->txtEditNoAktePerusahaan->Text%></p>
                                     </div>
                                </div>                                
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">Tanggal Akte Pendirian :</label>
                                     <div class="col-sm-10">
                                        <p class="form-control-static"><%=date('d M Y',$this->cmbEditTGLAktePendirian->TimeStamp)%></p>
                                     </div>
                                </div>                                
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">Nomor NPWP Perusahaan :</label>
                                     <div class="col-sm-10">
                                        <p class="form-control-static"><%=$this->txtEditNoNPWP->Text%></p>
