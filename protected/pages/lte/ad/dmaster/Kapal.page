            <prop:StartNavigationTemplate>                    
                <com:TLinkButton Text="Next" CommandName="NextStep" ValidationGroup="addKapal" CssClass="btn btn-default" />                    
            </prop:StartNavigationTemplate>
            <prop:StepNavigationTemplate>                
                <com:TLinkButton CausesValidation="False" CommandName="PreviousStep" Text="Previous" CssClass="btn btn-default" />               
                <com:TLinkButton Text="Next" CommandName="NextStep" ValidationGroup="addKapal" CssClass="btn btn-default" />                                
            </prop:StepNavigationTemplate>
            <prop:FinishNavigationTemplate>
                <com:TLinkButton Text="Complete" CommandName="Complete" ValidationGroup="addKapal" CssClass="btn btn-default" />                
            </prop:FinishNavigationTemplate>
