                            <i class="fa fa-minus"></i>
                        </button>
                        <button class="btn btn-primary btn-sm" onclick="pageredirect('<%=$this->constructUrl('dmaster.Kapal',true)%>');return false;">
                            <i class="fa fa-times"></i>
                        </button>
                    </div>
                </div>
